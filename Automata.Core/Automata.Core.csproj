<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageTags>automata; csharp; graph; finite-state-machine; dfa; nfa; fsa; minimize; minimization; </PackageTags>
    <Description>A light-weight library for creation, manipulation and minimization of finite-state automata</Description>
    <RepositoryUrl>https://github.com/HexMerlin/Automata</RepositoryUrl>
  </PropertyGroup>

</Project>
